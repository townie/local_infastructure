<p id="notice"><%= notice %></p>

<h1>Listing Prints</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Id</th>
      <th>Estimated print time</th>
      <th>Length</th>
      <th>Last</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prints.each do |print| %>
      <tr>
        <td><%= print.name %></td>
        <td><%= print.id %></td>
        <td><%= print.estimated_print_time %></td>
        <td><%= print.length %></td>
        <td><%= print.last %></td>
        <td><%= link_to 'Show', print %></td>
        <td><%= link_to 'Edit', edit_print_path(print) %></td>
        <td><%= link_to 'Destroy', print, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Print', new_print_path %>
